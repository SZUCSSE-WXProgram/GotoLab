@import"./style/icon.wxss";

/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  --themecolor:#5297F7;
}
.title {
  font-size: 40rpx;
  font-weight: 600;
  padding: 10rpx;
  margin-left: 10rpx;
  border-bottom: 2rpx solid rgb(220, 220, 220);
  height: 55rpx;
}

